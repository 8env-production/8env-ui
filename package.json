{
  "name": "@8env-ui/components",
  "version": "0.1.1",
  "description": "UI библиотека компонентов для React приложений с полной поддержкой TypeScript",
  "exports": {
    "./button": {
      "types": "./dist/components/Button/index.d.ts",
      "import": "./dist/components/Button/index.js",
      "require": "./dist/components/Button/index.js"
    },
    "./icon": {
      "types": "./dist/components/Icon/index.d.ts",
      "import": "./dist/components/Icon/index.js",
      "require": "./dist/components/Icon/index.js"
    },
    "./logo": {
      "types": "./dist/components/Logo/index.d.ts",
      "import": "./dist/components/Logo/index.js",
      "require": "./dist/components/Logo/index.js"
    },
    "./loader": {
      "types": "./dist/components/Loader/index.d.ts",
      "import": "./dist/components/Loader/index.js",
      "require": "./dist/components/Loader/index.js"
    },
    "./modal": {
      "types": "./dist/components/Modal/index.d.ts",
      "import": "./dist/components/Modal/index.js",
      "require": "./dist/components/Modal/index.js"
    },
    "./navigation": {
      "types": "./dist/components/Navigation/index.d.ts",
      "import": "./dist/components/Navigation/index.js",
      "require": "./dist/components/Navigation/index.js"
    },
    "./preview": {
      "types": "./dist/components/Preview/index.d.ts",
      "import": "./dist/components/Preview/index.js",
      "require": "./dist/components/Preview/index.js"
    },
    "./userpic": {
      "types": "./dist/components/UserPic/index.d.ts",
      "import": "./dist/components/UserPic/index.js",
      "require": "./dist/components/UserPic/index.js"
    },
    "./context": {
      "types": "./dist/context/index.d.ts",
      "import": "./dist/context/index.js",
      "require": "./dist/context/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./styles": "./dist/styles/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly && npm run build:styles && npm run inject-css",
    "build:styles": "sass src/styles/theme.scss dist/styles/index.css --no-source-map",
    "inject-css": "tsx scripts/inject-css-imports.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:scripts": "jest scripts/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/8env-production/8env-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "typescript",
    "ui-library",
    "react-components",
    "8env",
    "design-system"
  ],
  "author": "8env-production",
  "license": "ISC",
  "type": "module",
  "main": "./dist/context/index.js",
  "module": "./dist/context/index.js",
  "types": "./dist/context/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/8env-production/8env-ui/issues"
  },
  "homepage": "https://github.com/8env-production/8env-ui#readme",
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.3",
    "storybook": "^8.6.14",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  }
}
